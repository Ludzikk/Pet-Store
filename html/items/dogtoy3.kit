<!DOCTYPE html>
<html lang="en">
	<head>
		<!-- @include "../meta.kit" -->
		<title>Pet Store - Aquatel Starter Kit</title>
		<!-- @include "../link.kit" -->
		<link rel="stylesheet" href="../dist/css/style.min.css" />
		<script
			src="https://kit.fontawesome.com/bdb2f8ae99.js"
			crossorigin="anonymous"
		></script>
	</head>
	<body>
		<!-- @include "../navsubpages.kit" -->
		<main class="main">
			<div class="wrapper">
				<div class="main__item">
					<div class="main__item-left">
						<img
							class="main__item-img"
							src="../dist/img/dogtoy2.jpg"
							alt="Pet Nova Ball"
						/>
					</div>
					<div class="main__item-right">
						<p class="main__item-company">Pet Nova</p>
						<p class="main__item-desc">Best cheap ball for dogs</p>
						<div class="main__item-rate">
							<div class="main__item-rate-bg"></div>
							<i class="fa-solid fa-star main__item-icon"></i>
							<i class="fa-solid fa-star main__item-icon"></i>
							<i class="fa-solid fa-star main__item-icon"></i>
							<i class="fa-solid fa-star main__item-icon"></i>
							<i class="fa-solid fa-star main__item-icon"></i>
						</div>
						<p class="main__item-price">0.49$</p>
						<button class="main__button main__button--cart">
							<p class="main__button-text">add to cart</p>
						</button>
					</div>
				</div>
			</div>
		</main>
		<!-- @include "../footer.kit" -->
		<script src="../dist/js/index.min.js"></script>
		<script src="../dist/js/setrate.min.js"></script>
	</body>
</html>
