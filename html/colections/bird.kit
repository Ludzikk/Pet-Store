<!DOCTYPE html>
<html lang="en">
	<head>
		<!-- @include "../meta.kit" -->
		<title>Pet Store - Bird</title>
		<!-- @include "../link.kit" -->
		<link rel="stylesheet" href="../dist/css/style.min.css" />
		<script
			src="https://kit.fontawesome.com/bdb2f8ae99.js"
			crossorigin="anonymous"
		></script>
	</head>
	<body>
        <!-- @include "../navsubpages.kit" -->
        <!-- @include "../header.kit" -->
        <main class="main">
			<div class="wrapper">
				<div class="main__category-products-top">
					<p class="main__productscount">dfasd</p>
					<select class="main__select" name="sort" id="sort">
						<option value="az">Alphabetically, A-Z</option>
						<option value="za">Alphabetically, Z-A</option>
						<option value="lowtohigh">Price low to high</option>
						<option value="hightolow">Price high to low</option>
					</select>
				</div>
			<ul class="main__itemlist">
				<a href="../items/birdfood.html" class="main__itemlist-item">
					<img class="main__itemlist-img" src="../dist/img/birdfood.jpg" alt="Nutri Berries Food For Parrots">
					<p class="main__itemlist-name">Nutri berries - Food for parrots</p>
					<p class="main__itemlist-price">8.99$</p>
				</a>
				<a href="../items/birdfood2.html" class="main__itemlist-item">
					<img class="main__itemlist-img" src="../dist/img/birdfood1.jpg" alt="Ham Stake Pizza For Birds">
					<p class="main__itemlist-name">Ham stake - pizza for birds</p>
					<p class="main__itemlist-price">10.49$</p>
				</a>
				<a href="../items/birdfood3.html" class="main__itemlist-item">
					<img class="main__itemlist-img" src="../dist/img/birdfood2.jpg" alt="Versele Laga Food For Parrots With Fruits">
					<p class="main__itemlist-name">Versele Laga - Food For Parrots With Fruits</p>
					<p class="main__itemlist-price">7.00$</p>
				</a>
				<a href="../items/birdfood4.html" class="main__itemlist-item">
					<img class="main__itemlist-img" src="../dist/img/birdfood4.jpg" alt="Mix Of Seeds">
					<p class="main__itemlist-name">Mix of seeds for birds</p>
					<p class="main__itemlist-price">5.49$</p>
				</a>
			</ul>
		</div>
		</main>
        <!-- @include "../footer.kit" -->
        <script src="../dist/js/index.min.js"></script>
		<script src="../dist/js/header.min.js"></script>
    </body>
</html>
