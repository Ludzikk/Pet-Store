<!DOCTYPE html>
<html lang="en">
	<head>
		<!-- @include "../meta.kit" -->
		<title>Pet Store - Aquarium & Pond</title>
		<!-- @include "../link.kit" -->
		<link rel="stylesheet" href="../dist/css/style.min.css" />
		<script
			src="https://kit.fontawesome.com/bdb2f8ae99.js"
			crossorigin="anonymous"
		></script>
	</head>
	<body>
		<!-- @include "../navsubpages.kit" -->
		<!-- @include "../header.kit" -->
		<main class="main">
			<div class="wrapper">
				<div class="main__category-products-top">
					<p class="main__productscount">dfasd</p>
					<select class="main__select" name="sort" id="sort">
						<option value="az">Alphabetically, A-Z</option>
						<option value="za">Alphabetically, Z-A</option>
						<option value="lowtohigh">Price low to high</option>
						<option value="hightolow">Price high to low</option>
					</select>
				</div>
				<ul class="main__itemlist">
					<a href="#" class="main__itemlist-item">
						<img
							class="main__itemlist-img"
							src="../dist/img/aquarium.png"
							alt="Aquatel Aquarium Starter Kit"
						/>
						<p class="main__itemlist-name">Aquatel - starter kit</p>
						<p class="main__itemlist-price">29.99$</p>
					</a>
					<a href="#" class="main__itemlist-item">
						<img
							class="main__itemlist-img"
							src="../dist/img/aquarium2.png"
							alt="Aqua One Aquarium Starter Kit"
						/>
						<p class="main__itemlist-name">Aqua one - cheap aquarium starter kit</p>
						<p class="main__itemlist-price">16.49$</p>
					</a>
					<a href="#" class="main__itemlist-item">
						<img
							class="main__itemlist-img"
							src="../dist/img/fishfood1.jfif"
							alt="Tetra Goldfish Food"
						/>
						<p class="main__itemlist-name">Tetra - gold fish food</p>
						<p class="main__itemlist-price">4.99$</p>
					</a>
					<a href="#" class="main__itemlist-item">
						<img
							class="main__itemlist-img"
							src="../dist/img/fishfood2.jfif"
							alt="Nutrilogic Tropical Fish Food"
						/>
						<p class="main__itemlist-name">Nutrilogic - tropical fish food</p>
						<p class="main__itemlist-price">8.49$</p>
					</a>
					<a href="#" class="main__itemlist-item">
						<img
							class="main__itemlist-img"
							src="../dist/img/aquariumfilter.jpg"
							alt="Aquatel Aquarium Filter"
						/>
						<p class="main__itemlist-name">Aquatel - Aquarium Filter</p>
						<p class="main__itemlist-price">12.00$</p>
					</a>
					<a href="#" class="main__itemlist-item">
						<img
							class="main__itemlist-img"
							src="../dist/img/aquariumfilter2.jpg"
							alt="Aquatel Aquarium Filter"
						/>
						<p class="main__itemlist-name">Aquatel - Premium aquarium filter</p>
						<p class="main__itemlist-price">25.10$</p>
					</a>
				</ul>
			</div>
		</main>
		<!-- @include "../footer.kit" -->
		<script src="../dist/js/index.min.js"></script>
	</body>
</html>
