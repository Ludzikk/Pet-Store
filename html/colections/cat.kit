<!DOCTYPE html>
<html lang="en">
	<head>
		<!-- @include "../meta.kit" -->
		<title>Pet Store - Cat</title>
		<!-- @include "../link.kit" -->
		<link rel="stylesheet" href="../dist/css/style.min.css" />
		<script
			src="https://kit.fontawesome.com/bdb2f8ae99.js"
			crossorigin="anonymous"
		></script>
	</head>
	<body>
        <!-- @include "../navsubpages.kit" -->
        <!-- @include "../header.kit" -->
        <main class="main">
			<div class="wrapper">
				<div class="main__category-products-top">
					<p class="main__productscount">dfasd</p>
					<select class="main__select" name="sort" id="sort">
						<option value="az">Alphabetically, A-Z</option>
						<option value="za">Alphabetically, Z-A</option>
						<option value="lowtohigh">Price low to high</option>
						<option value="hightolow">Price high to low</option>
					</select>
				</div>
			<ul class="main__itemlist">
				<a href="../items/catfood.html" class="main__itemlist-item">
					<img class="main__itemlist-img" src="../dist/img/catfood.png" alt="Reveal Cat Food">
					<p class="main__itemlist-name">Reveal - best food for your cat</p>
					<p class="main__itemlist-price">15.99$</p>
				</a>
				<a href="../items/catlitter.html" class="main__itemlist-item">
					<img class="main__itemlist-img" src="../dist/img/catsand.jpg" alt="Cats Best Cat Litter">
					<p class="main__itemlist-name">Cats best - best cat litter</p>
					<p class="main__itemlist-price">20.99$</p>
				</a>
				<a href="../items/catlitter2.html" class="main__itemlist-item">
					<img class="main__itemlist-img" src="../dist/img/catsand2.jpg" alt="Calitti Cat Litter">
					<p class="main__itemlist-name">calitti - cheap cat litter</p>
					<p class="main__itemlist-price">12.99$</p>
				</a>
				<a href="../items/cattoy.html" class="main__itemlist-item">
					<img class="main__itemlist-img" src="../dist/img/cattoy1.jpg" alt="Catit Toys Labyrynth For Cat">
					<p class="main__itemlist-name">Catit - your cat won't be bored</p>
					<p class="main__itemlist-price">23.29$</p>
				</a>
				<a href="../items/cattoy2.html" class="main__itemlist-item">
					<img class="main__itemlist-img" src="../dist/img/cattoy.jpg" alt="Moehui cat toy">
					<p class="main__itemlist-name">Moehui - have fun with your cat</p>
					<p class="main__itemlist-price">5.49$</p>
				</a>
			</ul>
		</div>
		</main>
        <!-- @include "../footer.kit" -->
		<script src="../dist/js/index.min.js"></script>
		<script src="../dist/js/header.min.js"></script>
