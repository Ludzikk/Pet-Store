<!DOCTYPE html>
<html lang="en">
	<head>
		<!-- @include "../meta.kit" -->
		<title>Pet Store - Reptile</title>
		<!-- @include "../link.kit" -->
		<link rel="stylesheet" href="../dist/css/style.min.css" />
		<script
			src="https://kit.fontawesome.com/bdb2f8ae99.js"
			crossorigin="anonymous"
		></script>
	</head>
	<body>
        <!-- @include "../navsubpages.kit" -->
        <!-- @include "../header.kit" -->
        <main class="main">
			<div class="wrapper">
				<div class="main__category-products-top">
					<p class="main__productscount">dfasd</p>
					<select name="sort" id="sort">
						<option value="az">Alphabetically, A-Z</option>
						<option value="za">Alphabetically, Z-A</option>
						<option value="lowtohigh">Price low to high</option>
						<option value="hightolow">Price high to low</option>
					</select>
				</div>
			<ul class="main__itemlist">
				<a href="#" class="main__itemlist-item">
					<img class="main__itemlist-img" src="../dist/img/dogball.jpg" alt="Dog Ball">
					<p class="main__itemlist-name">Jolly Ball - Blue Color</p>
					<p class="main__itemlist-price">4.99$</p>
				</a>
				<a href="#" class="main__itemlist-item">
					<img class="main__itemlist-img" src="../dist/img/pedigree.png" alt="Pedigree Food For Dogs">
					<p class="main__itemlist-name">Pedigree - Full Kit</p>
					<p class="main__itemlist-price">14.49$</p>
				</a>
				<a href="#" class="main__itemlist-item">
					<img class="main__itemlist-img" src="../dist/img/dogcollar.jpg" alt="Esme Dog Collar">
					<p class="main__itemlist-name">Esme - Premium Dog Collar</p>
					<p class="main__itemlist-price">10.00$</p>
				</a>
				<a href="#" class="main__itemlist-item">
					<img class="main__itemlist-img" src="../dist/img/dogcolars.jpg" alt="Glamdog Dog Collars">
					<p class="main__itemlist-name">Glamdog - Cheap Dog Collars</p>
					<p class="main__itemlist-price">2.49$</p>
				</a>
				<a href="#" class="main__itemlist-item">
					<img class="main__itemlist-img" src="../dist/img/dogharness.jpg" alt="Dog Harness">
					<p class="main__itemlist-name">Crazy Bananas - Dog Harness</p>
					<p class="main__itemlist-price">10.29$</p>
				</a>
				<a href="#" class="main__itemlist-item">
					<img class="main__itemlist-img" src="../dist/img/dogtoy1.jpg" alt="Trixie Dog Toy">
					<p class="main__itemlist-name">Trixie - dog toy</p>
					<p class="main__itemlist-price">1.49$</p>
				</a>
				<a href="#" class="main__itemlist-item">
					<img class="main__itemlist-img" src="../dist/img/dogtoy2.jpg" alt="Pet Nova Ball">
					<p class="main__itemlist-name">pet nova - best cheap ball for dogs</p>
					<p class="main__itemlist-price">0.49$</p>
				</a>
				<a href="#" class="main__itemlist-item">
					<img class="main__itemlist-img" src="../dist/img/dogtoy3.jpg" alt="Beco Pets Toy For Dog">
					<p class="main__itemlist-name">beco pets - toy for your best friend</p>
					<p class="main__itemlist-price">3.49$</p>
				</a>
			</ul>
		</div>
		</main>
        <!-- @include "../footer.kit" -->
        <script src="../dist/js/index.min.js"></script>
    </body>
</html>
