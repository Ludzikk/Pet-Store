<!DOCTYPE html>
<html lang="en">
	<head>
		<!-- @include "../meta.kit" -->
		<title>Pet Store - Reptile</title>
		<!-- @include "../link.kit" -->
		<link rel="stylesheet" href="../dist/css/style.min.css" />
		<script
			src="https://kit.fontawesome.com/bdb2f8ae99.js"
			crossorigin="anonymous"
		></script>
	</head>
	<body>
        <!-- @include "../navsubpages.kit" -->
        <!-- @include "../header.kit" -->
        <main class="main">
			<div class="wrapper">
				<div class="main__category-products-top">
					<p class="main__productscount">dfasd</p>
					<select class="main__select" name="sort" id="sort">
						<option value="az">Alphabetically, A-Z</option>
						<option value="za">Alphabetically, Z-A</option>
						<option value="lowtohigh">Price low to high</option>
						<option value="hightolow">Price high to low</option>
					</select>
				</div>
			<ul class="main__itemlist">
				<a href="#" class="main__itemlist-item">
					<img class="main__itemlist-img" src="../dist/img/reptile1.jpg" alt="Lucky Reptile Hideout">
					<p class="main__itemlist-name">Lukcy reptile - hideout for reptiles</p>
					<p class="main__itemlist-price">9.90$</p>
				</a>
				<a href="#" class="main__itemlist-item">
					<img class="main__itemlist-img" src="../dist/img/reptilefood1.jpg" alt="Habistat Food For Reptile">
					<p class="main__itemlist-name">Habistat - Food For Reptile</p>
					<p class="main__itemlist-price">10.49$</p>
				</a>
				<a href="#" class="main__itemlist-item">
					<img class="main__itemlist-img" src="../dist/img/reptilefood2.jpg" alt="Arcadia Food For Reptiler">
					<p class="main__itemlist-name">Arcadia - Food For Reptile</p>
					<p class="main__itemlist-price">10.00$</p>
				</a>
				<a href="#" class="main__itemlist-item">
					<img class="main__itemlist-img" src="../dist/img/reptileanimal1.jpg" alt="Physignathus cocincinus">
					<p class="main__itemlist-name">Physignathus cocincinus</p>
					<p class="main__itemlist-price">25.99$</p>
				</a>
				<a href="#" class="main__itemlist-item">
					<img class="main__itemlist-img" src="../dist/img/reptileanimal2.jpeg" alt="Boa constrictor">
