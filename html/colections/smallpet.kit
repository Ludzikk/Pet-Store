<!DOCTYPE html>
<html lang="en">
	<head>
		<!-- @include "../meta.kit" -->
		<title>Pet Store - Small Pet</title>
		<!-- @include "../link.kit" -->
		<link rel="stylesheet" href="../dist/css/style.min.css" />
		<script
			src="https://kit.fontawesome.com/bdb2f8ae99.js"
			crossorigin="anonymous"
		></script>
	</head>
	<body>
		<!-- @include "../navsubpages.kit" -->
		<!-- @include "../header.kit" -->
		<main class="main">
			<div class="wrapper">
				<div class="main__category-products-top">
					<p class="main__productscount">dfasd</p>
					<select class="main__select" name="sort" id="sort">
						<option value="az">Alphabetically, A-Z</option>
						<option value="za">Alphabetically, Z-A</option>
						<option value="lowtohigh">Price low to high</option>
						<option value="hightolow">Price high to low</option>
					</select>
				</div>
				<ul class="main__itemlist">
					<a href="../items/smallpet.html" class="main__itemlist-item">
						<img
							class="main__itemlist-img"
							src="../dist/img/smallpet1.jpg"
							alt="Giant Water Feeder"
						/>
						<p class="main__itemlist-name">Giant - Water Feeder 1100ml</p>
						<p class="main__itemlist-price">4.99$</p>
					</a>
					<a href="../items/smallpet2.html" class="main__itemlist-item">
						<img
							class="main__itemlist-img"
							src="../dist/img/smallpet2.jpg"
