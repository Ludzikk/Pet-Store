<!DOCTYPE html>
<html lang="en">
	<head>
		<!-- @include "../meta.kit" -->
		<title>Pet Store - Small Pet</title>
		<!-- @include "../link.kit" -->
		<link rel="stylesheet" href="../dist/css/style.min.css" />
		<script
			src="https://kit.fontawesome.com/bdb2f8ae99.js"
			crossorigin="anonymous"
		></script>
	</head>
	<body>
        <!-- @include "../navsubpages.kit" -->
        <!-- @include "../header.kit" -->
        <main class="main">
			<div class="wrapper">
				<div class="main__category-products-top">
					<p class="main__productscount">dfasd</p>
					<select class="main__select" name="sort" id="sort">
						<option value="az">Alphabetically, A-Z</option>
						<option value="za">Alphabetically, Z-A</option>
						<option value="lowtohigh">Price low to high</option>
						<option value="hightolow">Price high to low</option>
					</select>
				</div>
			<ul class="main__itemlist">
				<a href="#" class="main__itemlist-item">
					<img class="main__itemlist-img" src="../dist/img/smallpet1.jpg" alt="Giant Water Feeder">
					<p class="main__itemlist-name">Giant - Water Feeder 1100ml</p>
					<p class="main__itemlist-price">4.99$</p>
				</a>
				<a href="#" class="main__itemlist-item">
					<img class="main__itemlist-img" src="../dist/img/smallpet2.jpg" alt="Ferplast Spinning Wheel">
					<p class="main__itemlist-name">Ferplast - Spinning Wheel</p>
					<p class="main__itemlist-price">6.49$</p>
				</a>
				<a href="#" class="main__itemlist-item">
					<img class="main__itemlist-img" src="../dist/img/smallpet3.jpg" alt="Trixie Spinning Wheel">
					<p class="main__itemlist-name">Trixie - Spinning Wheel</p>
					<p class="main__itemlist-price">9.00$</p>
				</a>
				<a href="#" class="main__itemlist-item">
					<img class="main__itemlist-img" src="../dist/img/smallpet4.jpg" alt="Ferplast Bowl">
					<p class="main__itemlist-name">ferplast - bowl</p>
					<p class="main__itemlist-price">2.49$</p>
				</a>
				<a href="#" class="main__itemlist-item">
					<img class="main__itemlist-img" src="../dist/img/smallpet5.jpg" alt="Ferplast Cage">
					<p class="main__itemlist-name">Ferplast - cage 107,5x72x50cm</p>
					<p class="main__itemlist-price">30.29$</p>
				</a>
				<a href="#" class="main__itemlist-item">
					<img class="main__itemlist-img" src="../dist/img/smallpet6.jpg" alt="Ferplast Cage">
					<p class="main__itemlist-name">Ferplast - Cage 78x48x70cm</p>
					<p class="main__itemlist-price">25.49$</p>
				</a>
				<a href="#" class="main__itemlist-item">
					<img class="main__itemlist-img" src="../dist/img/smallpet8.jpg" alt="deli nature food for hamster">
					<p class="main__itemlist-name">deli nature - food for hamster</p>
					<p class="main__itemlist-price">5.49$</p>
				</a>
				<a href="#" class="main__itemlist-item">
					<img class="main__itemlist-img" src="../dist/img/smallpet7.png" alt="mixerama food for hamster">
					<p class="main__itemlist-name">mixerama - food for hamster</p>
					<p class="main__itemlist-price">3.49$</p>
				</a>
				<a href="#" class="main__itemlist-item">
					<img class="main__itemlist-img" src="../dist/img/smallpet9.jpg" alt="Chipsi sawdust">
					<p class="main__itemlist-name">Chipsi - sawdust</p>
					<p class="main__itemlist-price">4.49$</p>
				</a>
			</ul>
		</div>
		</main>
        <!-- @include "../footer.kit" -->
        <script src="../dist/js/index.min.js"></script>
    </body>
</html>
